<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean class="com.springCore.one_setterInjection.collections.Employee" name="employee1">

        <property name="name" value="Bishajit"/>
        <property name="phone">
            <list>
                <value>01700000000</value>
                <value>01800000000</value>
                <value>01900000000</value>
                <value>01300000000</value>
            </list>
        </property>
        <property name="address">
            <set>
                <value>address1</value>
                <value>address2</value>
                <value>address3</value>
                <value>address4</value>
            </set>
        </property>
        <property name="courses">
            <map>
                <entry key="java" value="2 month"/>
                <entry key="python" value="3 month"/>
                <entry key="C" value="2 month"/>
                <entry key="C++" value="4 month"/>
            </map>
        </property>
        <property name="properties">
            <props>
                <prop key="props1">properties1</prop>
                <prop key="props2">properties2</prop>
                <prop key="props3">properties3</prop>
                <prop key="props4">properties4</prop>
            </props>
        </property>

    </bean>


</beans>